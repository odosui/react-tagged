{
  "name": "react-tagged",
  "version": "1.2.0",
  "module": "dist/react-tagged.mjs",
  "main": "dist/react-tagged.umd.js",
  "exports": {
    ".": {
      "import": "./dist/react-tagged.mjs",
      "require": "./dist/react-tagged.umd.js"
    },
    "./dist/index.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "dist/index.d.ts",
  "description": "A robust tags input for React with TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/odosui/react-tagged.git"
  },
  "scripts": {
    "test": "vitest --run",
    "test:watch": "jest --watch",
    "build": "rm -rf dist && vite build",
    "types": "tsc --project tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --output-dir react-tagged --disable-telemetry"
  },
  "peerDependencies": {
    "react": ">= 16.8"
  },
  "keywords": [
    "react",
    "tags"
  ],
  "author": "weareodos",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@storybook/addon-essentials": "^8.3.4",
    "@storybook/addon-interactions": "^8.3.4",
    "@storybook/addon-links": "^8.3.4",
    "@storybook/addon-onboarding": "^8.3.4",
    "@storybook/blocks": "^8.3.4",
    "@storybook/react": "^8.3.4",
    "@storybook/react-vite": "^8.3.4",
    "@storybook/test": "^8.3.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "happy-dom": "^15.7.4",
    "react": ">= 16.8",
    "react-dom": ">= 16.8",
    "sass": "^1.37.5",
    "storybook": "^8.3.4",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.2",
    "vitest": "^2.1.1"
  }
}
